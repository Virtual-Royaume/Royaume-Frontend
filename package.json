{
  "name": "royaume-frontend",
  "version": "0.2.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "test:unit": "vitest --ui",
    "test:e2e": "playwright test",
    "sb:dev": "storybook dev -p 6006",
    "sb:build": "storybook build",
    "sb:start": "serve storybook-static",
    "ts:check": "tsc --noEmit",
    "preinstall": "npx only-allow pnpm"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.1.1",
    "@tanstack/react-query": "^4.29.13",
    "@tw-classed/react": "^1.5.1",
    "@types/node": "20.3.1",
    "@types/react": "18.2.12",
    "@types/react-dom": "18.2.5",
    "clsx": "^1.2.1",
    "dayjs": "^1.11.8",
    "next": "13.4.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.44.3",
    "react-icons": "^4.9.0",
    "typescript": "5.1.3",
    "usehooks-ts": "^2.9.1",
    "zod": "^3.21.4",
    "zustand": "^4.3.8"
  },
  "devDependencies": {
    "@bluzzi/eslint-config": "^1.1.0",
    "@playwright/test": "^1.35.1",
    "@storybook/addon-essentials": "^7.0.21",
    "@storybook/addon-interactions": "^7.0.21",
    "@storybook/addon-links": "^7.0.21",
    "@storybook/manager-api": "^7.0.21",
    "@storybook/nextjs": "^7.0.21",
    "@storybook/react": "^7.0.21",
    "@storybook/testing-library": "^0.1.0",
    "@testing-library/react": "^14.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/ui": "^0.32.0",
    "autoprefixer": "^10.4.14",
    "dotenv": "^16.1.4",
    "eslint": "8.42.0",
    "eslint-config-next": "^13.4.6",
    "happy-dom": "^9.20.3",
    "postcss": "^8.4.24",
    "serve": "^14.2.0",
    "sharp": "^0.32.1",
    "storybook": "^7.0.21",
    "tailwindcss": "^3.3.2",
    "vite": "^4.3.9",
    "vitest": "^0.32.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "eslintConfig": {
    "extends": [
      "next/core-web-vitals",
      "@bluzzi"
    ],
    "rules": {
      "react-hooks/exhaustive-deps": "off",
      "react/jsx-curly-brace-presence": [
        "error",
        {
          "props": "never",
          "children": "never"
        }
      ]
    }
  }
}
